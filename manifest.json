{
  "manifest_version": 3,
  "name": "JustPlay",
  "version": "1.0",
  "description": "To play Imdb movie in VidSrc",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.imdb.com/*","https://www.themoviedb.org/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}